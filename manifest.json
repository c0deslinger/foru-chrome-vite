{
  "manifest_version": 3,
  "name": "ForU AI",
  "description": "A new way to exist on-chain.",
  "environment" : "prod",
  "foruConfigProd": {
    "secretKey": "5452ea6c10f23d4e5b83d1eb393f99d53705951d7b43acc90eadf17ecc839623",
    "privateKey": "7886064592c4c35255318389dbe15226de78f16a46f809a29218bc43ea0f8684",
    "apiBaseUrl": "https://api-social.foruai.io"
  },
  "foruConfigDev": {
    "secretKey": "9f1c8792622196d8d607acba3c7a117ab30ad381b88424f6ed5ca7c72b048364",
    "privateKey": "6587e29c8bf629eab6d29eaf1009450d10f05dd35b2ed9d9eb2b60c65b78638b",
    "apiBaseUrl": "https://api-social-dev.foruai.io"
  },
  "foruConfigStaging": {
    "secretKey": "9f1c8792622196d8d607acba3c7a117ab30ad381b88424f6ed5ca7c72b048364",
    "privateKey": "6587e29c8bf629eab6d29eaf1009450d10f05dd35b2ed9d9eb2b60c65b78638b",
    "apiBaseUrl": "https://api-social-demo.foruai.io"
  },
  "action": {
    "default_icon": {
      "16": "icon-16.png",
      "32": "icon-32.png",
      "48": "icon-48.png",
      "128": "icon-128.png"
    }
  },
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "sidePanel",
    "webNavigation",
    "storage"
  ],
  "host_permissions": [
    "*://*.twitter.com/*",
    "*://twitter.com/*",
    "*://*.x.com/*",
    "*://x.com/*"
  ],
  "side_panel": {
    "default_path": "src/pages/panel/index.html",
    "matches": [
      "*://*.twitter.com/*",
      "*://twitter.com/*",
      "*://*.x.com/*",
      "*://x.com/*",
      "https://generativelanguage.googleapis.com/*"
    ]
  },
  "background": {
    "service_worker": "src/pages/background/index.ts"
  },
  "content_scripts": [
    {
      "matches": ["*://twitter.com/*", "*://x.com/*"],
      "js": [
        "src/pages/content/index.tsx"
      ],
      "css": [
        "contentStyle.css"
      ],
      "run_at": "document_end"
    }
  ],
  "devtools_page": "src/pages/devtools/index.html",
  "web_accessible_resources": [
    {
      "resources": [
        "contentStyle.css",
        "icon-128.png",
        "icon-32.png",
        "icon-16.png",
        "icon-48.png",
        "images/*",
        "assets/*",
        "src/pages/content/*/index.css",
        "src/pages/popup/*/index.css"
      ],
      "matches": ["*://*.x.com/*", "*://*.twitter.com/*", "*://twitter.com/*"]
    }
  ]
}
